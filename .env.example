# PPRA Tender Alerts - Environment Variables Template
# Copy this file to .env and fill in your actual values

# PPRA API Configuration
PPRA_API_URL=https://ppra.gov.pk/api/tenders
PPRA_API_KEY=your_api_key_here

# Django Settings (if using Django backend)
SECRET_KEY=your_secret_key_here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Configuration (if using PostgreSQL)
DB_NAME=ppra_tender_db
DB_USER=ppra_user
DB_PASSWORD=your_password_here
DB_HOST=localhost
DB_PORT=5432

# Twilio Configuration (optional, for SMS alerts)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
